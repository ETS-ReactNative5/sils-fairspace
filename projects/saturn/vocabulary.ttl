    ],
    [
        sh:name "Title" ;
        sh:description "The title of the investigation." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:investigationTitle
    ],
    [
        sh:name "Description" ;
        sh:description "The description of the investigation." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
