        sh:path sils:department
    ],
    [
        sh:name "Studies" ;
        sh:description "Linked studies" ;
        sh:path [sh:inversePath sils:investigation]
    ],
    [
        sh:name "Files" ;
        sh:description "Linked files" ;
        sh:path [sh:inversePath sils:aboutInvestigation]
