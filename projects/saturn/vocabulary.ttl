sils:Sample a rdfs:Class, sh:NodeShape ;
    sh:closed false ;
    sh:description "A biological sample taken from an observation unit that can potentially be processed further to acquire data from. A multitude of samples can be taken from one or more observation units when for example performing time-scale experiments or sampling from different regions of the observation unit." ;
    sh:name "Sample" ;
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    sh:property
    [
        sh:name "Label" ;
        sh:description "Unique sample label." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        dash:singleLine true ;
        fs:importantProperty true ;
        sh:path rdfs:label
    ],
    [
        sh:name "Identifier" ;
        sh:description "The identifier of the sample." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:sampleIdentifier
    ],
    [
        sh:name "Name" ;
        sh:description "The readable name of the sample." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:sampleName
    ],
    [
        sh:name "Description" ;
        sh:description "The description of the sample." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:sampleDescription
    ],
    [
        sh:name "Observation unit" ;
        sh:description "The observation unit this sample belongs to." ;
        sh:maxCount 1 ;
        sh:class sils:ObservationUnit ;
        sh:path sils:observationUnit
    ],
    [
        sh:name "Local storage location" ;
        sh:description "The location of where this sample is stored locally." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:localStorageLocation
    ],
    # [
    #     sh:name "Taxonomy ID" ;
    #     sh:description "The taxonomic identifier of the sample according to https://www.uniprot.org/taxonomy." ;
    #     sh:maxCount 1 ;
    #     sh:class sils:TaxonomyIDs ;
    #     sh:path sils:taxonomyId
    # ],
    [
        sh:name "Sample organism" ;
        sh:description "The species of the sample." ;
        sh:maxCount 1 ;
        sh:class sils:Species ;
        sh:path sils:isOfSpecies
    ],
    [
        sh:name "Host" ;
        sh:description "If applicable, the host name of which the sample was obtained." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:host
    ],
    [
        sh:name "Biosafety level" ;
        sh:description "The biosafety level of the sample." ;
        sh:datatype xsd:string ;    # It can be BSL-1, BSL-2 or 1, 2, thus can't be an integer.
        sh:maxCount 1 ;
        sh:path sils:bioSafetyLevel
    ],
    [
        sh:name "Sample size" ;
        sh:description "The size of the sample (ml, grams)." ;
        sh:datatype xsd:decimal ;
        sh:maxCount 1 ;
        sh:path sils:sampleSize
    ],
    [
        sh:name "Sample condition" ;
        sh:description "The condition of the sample." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:sampleCondition
    ],
    [
        sh:name "Material" ;
        sh:description "The material of which the sample is composed of." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:material
    ],
    [
        sh:name "Sampling strategy" ;
        sh:description "The strategy used to perform the sampling procedure." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:samplingStrategy
    ],
    [
        sh:name "Sample treatment" ;
        sh:description "The treatment applied to the sample." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:sampleTreatment
    ],
    [
        sh:name "Collection date" ;
        sh:description "The date when this sample was collected (yyyy-mm-dd, ISO 8601)." ;
        sh:datatype xsd:date ;
        sh:maxCount 1 ;
        sh:path sils:collectionDate
    ],
    [
        sh:name "Collection time" ;
        sh:description "The time this sample was collected (24h notation)." ;
        sh:datatype xsd:integer ;
        sh:maxCount 1 ;
        sh:path sils:collectionTime
    ],
    [
        sh:name "Time point" ;
