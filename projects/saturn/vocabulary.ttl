## Augmented system class shapes

# Project               > File, Directory, Collection
# Investigation         > File, Directory, Collection
# Study                 > File, Directory, Collection
# Observation unit      > File
# Sample                > File
# Assay                 > File

sils:aboutProject a rdf:Property .
sils:aboutInvestigation a rdf:Property .
sils:aboutStudy a rdf:Property .
sils:aboutObservationUnit a rdf:Property .
sils:aboutSample a rdf:Property .
sils:aboutAssay a rdf:Property .

fs:File sh:property
    [
        sh:name "Is about project" ;
        sh:description "Projects that are featured in this collection." ;
        sh:class sils:Project ;
        sh:path sils:aboutProject
    ],
    [
        sh:name "Is about investigation" ;
        sh:description "Investigations that are featured in this collection." ;
        sh:class sils:Investigation ;
        sh:path sils:aboutInvestigation
    ],
    [
        sh:name "Is about study" ;
        sh:description "Studies that are featured in this collection." ;
        sh:class sils:Study ;
        sh:path sils:aboutStudy
    ],
    [
        sh:name "Is about observation unit" ;
        sh:description "Observation units that are featured in this collection." ;
        sh:class sils:ObservationUnit ;
        sh:path sils:aboutObservationUnit
    ],
    [
        sh:name "Is about sample" ;
        sh:description "Samples that are featured in this collection." ;
        sh:class sils:Sample ;
        sh:path sils:aboutSample
    ],
    [
        sh:name "Is about assay" ;
        sh:description "Assays that are featured in this collection." ;
        sh:class sils:Assay ;
        sh:path sils:aboutAssay
    ] .

fs:Directory sh:property
    [
        sh:name "Is about project" ;
        sh:description "Projects that are featured in this collection." ;
        sh:class sils:Project ;
        sh:path sils:aboutProject
    ],
    [
        sh:name "Is about investigation" ;
        sh:description "Investigations that are featured in this collection." ;
        sh:class sils:Investigation ;
        sh:path sils:aboutInvestigation
    ],
    [
        sh:name "Is about study" ;
        sh:description "Studies that are featured in this collection." ;
        sh:class sils:Study ;
        sh:path sils:aboutStudy
    ] .

fs:Collection sh:property
    [
        sh:name "Is about project" ;
        sh:description "Projects that are featured in this collection." ;
        sh:class sils:Project ;
        sh:path sils:aboutProject
    ],
    [
        sh:name "Is about investigation" ;
        sh:description "Investigations that are featured in this collection." ;
        sh:class sils:Investigation ;
        sh:path sils:aboutInvestigation
    ],
    [
        sh:name "Is about study" ;
        sh:description "Studies that are featured in this collection." ;
        sh:class sils:Study ;
        sh:path sils:aboutStudy
    ] .

## User class Shapes

sils:projectIdentifier a rdf:Property .
sils:projectTitle a rdf:Property .
sils:projectDescription a rdf:Property .

sils:Project a rdfs:Class, sh:NodeShape ;
    sh:closed false ;
    sh:description "A research programme or a grant-funded programme of work." ;
    sh:name "Project" ;
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    sh:property
    [
        sh:name "Label" ;
        sh:description "Unique project label." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        dash:singleLine true ;
        fs:importantProperty true ;
        sh:path rdfs:label
    ],
    [
        sh:name "Identifier" ;
        sh:description "The identifier of the project." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:projectIdentifier
    ],
    [
        sh:name "Title" ;
        sh:description "The title of the project." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:projectTitle
    ],
    [
        sh:name "Description" ;
        sh:description "The description of the project." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:projectDescription
    ],
    [
        sh:name "Investigations" ;
        sh:description "Linked investigations" ;
        sh:path [sh:inversePath sils:project]
    ],
    [
        sh:name "Files" ;
        sh:description "Linked files" ;
        sh:path [sh:inversePath sils:aboutProject]
    ] .

sils:Organization a rdfs:Class, sh:NodeShape ;
    sh:closed false ;
    sh:description "An organization is an entity – such as a company, an institution, or an association – comprising one or more people and having a particular purpose." ;
    sh:name "Organization" ;
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    sh:property
    [
        sh:name "Label" ;
        sh:description "Unique organization label." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        dash:singleLine true ;
        fs:importantProperty true ;
        sh:path rdfs:label
    ] .

sils:Department a rdfs:Class, sh:NodeShape ;
    sh:closed false ;
    sh:description "A division of a large organization such as a government, university, or business, dealing with a specific area of activity." ;
    sh:name "Department" ;
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    sh:property
    [
        sh:name "Label" ;
        sh:description "Unique department label." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        dash:singleLine true ;
        fs:importantProperty true ;
        sh:path rdfs:label
    ] .

