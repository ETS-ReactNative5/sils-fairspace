sils:ObservationUnit a rdfs:Class, sh:NodeShape ;
    sh:closed false ;
    sh:description "An observation unit is an object that is subject to instances of observation and measurement. For example in a study each living entity (animal or plant), bioreactor or area studied is one observation unit. When studying 50 different animals, each animal should become one observation unit." ;
    sh:name "Observation unit" ;
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    sh:property
    [
        sh:name "Label" ;
        sh:description "Unique observation unit label." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        dash:singleLine true ;
        fs:importantProperty true ;
        sh:path rdfs:label
    ],
    [
        sh:name "Identifier" ;
        sh:description "The identifier of the observation unit." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:observationUnitIdentifier
    ],
    [
        sh:name "Title" ;
        sh:description "The title of the observation unit." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:observationUnitTitle
    ],
    [
        sh:name "Description" ;
        sh:description "The description of the observation unit." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:observationUnitDescription
    ],
    [
        sh:name "Study" ;
        sh:description "The study this observation unit belongs to." ;
        sh:maxCount 1 ;
        sh:class sils:Study ;
        sh:path sils:study
    ],
    [
        sh:name "Country" ;
        sh:description "The country where this observation unit resides in." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:country
    ],
    [
        sh:name "Local region name" ;
        sh:description "The name of the local region where this observation unit resides in." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:localRegionName
    ],
    [
        sh:name "Continent" ;
        sh:description "The continent where this observation unit resides in." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:continent
    ],
    [
        sh:name "Latitude" ;
        sh:description "The latitude coordinates of where this observation unit is located." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:latitude
    ],
    [
        sh:name "Longitude" ;
        sh:description "The longitude coordinates of where this observation unit is located." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:longitude
    ],
    [
        sh:name "Altitude" ;
        sh:description "The altitude levels in meters (e.g. 1.5 or 100) of where this observation unit is located." ;
        sh:datatype xsd:decimal ;
        sh:maxCount 1 ;
        sh:path sils:altitude
    ],
    [
        sh:name "Depth" ;
        sh:description "The depth in meters (e.g. 0.1 or 4.5) of where this observation unit is located." ;
        sh:datatype xsd:decimal ;
        sh:maxCount 1 ;
        sh:path sils:depth
    ],
    [
        sh:name "Elevation" ;
        sh:description "The elevation levels in meters (e.g. 0.1 or 4.5) of where this observation unit is located." ;
        sh:datatype xsd:decimal ;
        sh:maxCount 1 ;
        sh:path sils:elevation
    ],
    [
        sh:name "Samples" ;
        sh:description "Linked samples" ;
        sh:path [sh:inversePath sils:observationUnit]
    ],
    [
        sh:name "Files" ;
        sh:description "Linked files" ;
        sh:path [sh:inversePath sils:aboutObservationUnit]
    ] .

