        sh:path sils:project
    ],
    [
        sh:name "First name" ;
        sh:description "The first name of the investigator." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path sils:firstName
    ],
    [
        sh:name "Last name" ;
